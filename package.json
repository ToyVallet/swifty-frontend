{
  "name": "swifty-frontend",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "dotenvx run -- turbo dev",
    "dev:root": "dotenvx run -- turbo dev --filter root-admin",
    "dev:user": "dotenvx run -- turbo dev --filter user-web",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "@swifty/eslint-config": "*",
    "@swifty/typescript-config": "*",
    "husky": "^7.0.2",
    "prettier": "^3.2.5",
    "turbo": "latest"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "yarn@1.22.21",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "dependencies": {
    "@dotenvx/dotenvx": "^0.44.1",
    "dotenv-cli": "^7.4.2"
  }
}
